@using QLNS.Model.IdentityEntities
@using QLNS.Web.Areas.RecruitmentRequestArea.Models;

@model RecruitmentRequestViewModel.RecruitmentRequestEditViewModel

@{ 
    int demo = 1;
}
<div class="form-group">
    <label class="control-label col-sm-4" for="Code">
        Biểu mẫu kỹ năng/yêu cầu <span class="red">*</span>
    </label>
    <div class="col-sm-8">
        @Html.DropDownListFor(m => m.TemplateId, Model.GroupTemplate,"--- Chọn biểu mẫu ---", new { @class = "form-control chosen-select tag-input-style" })
        @Html.ValidationMessageFor(m => m.TemplateId, "", new { @class = "text-danger" })
    </div>
</div>